<Window x:Class="Redefinable.Applications.EasyCapture.View.RectCaptureSelectToolWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Redefinable.Applications.EasyCapture.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" ResizeMode="NoResize" WindowState="Maximized" WindowStyle="None" Topmost="False">
    <Grid Name="MainGrid">
        <Image Name="BackImage" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Rectangle Name="SelectRect"  Fill="#667EB6E4" HorizontalAlignment="Left" Height="100" Stroke="#FF4A8EC7" VerticalAlignment="Top" Width="100" Visibility="Hidden"/>
    </Grid>
</Window>
